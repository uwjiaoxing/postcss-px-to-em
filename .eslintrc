{
  "rules": {
    "comma-dangle": [2, "always-multiline"],
    "curly": [0],
    "indent": [0],
    "key-spacing": [0],
    "no-extra-parens": [0],
    "no-loop-func": [0],
    "no-multi-spaces": [0],
    "quote-props": [0],
    "no-underscore-dangle": [0],
    "no-unused-expressions": [0],
    "no-unused-vars": [2],
    "quotes": [0],
    "strict": [0],
    "semi": [2]
  },
  "env": {
    "mocha": true,
    "node": true
  }
}
